    "axios": "^1.2.3",
    "bignumber.js": "^9.0.1",
    "ethers": "5.7.2",
    "web3": "^1.3.4"
  },
  "resolutions": {
    "@celo/connect": "^4.1.0",
    "@celo/contractkit": "^4.1.0",
    "web3": "1.3.6"
